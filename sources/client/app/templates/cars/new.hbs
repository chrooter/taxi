<div class="editor user-editor">
    <form class="form-horizontal">
      {{property-row title="Номерной знак" value=regNumber errors=errors.regNumber}}
      {{property-row title="Марка" value=make errors=errors.make}}
      {{property-row title="Модель" value=carModel errors=errors.carModel}}
      {{property-row title="Тариф" value=rate errors=errors.rate}}
      {{property-row title="Пробег, км" value=mileage errors=errors.mileage}}
      {{property-row title="Следующий ТО, км" value=service errors=errors.service}}
      {{property-row title="Примечания" value=comment errors=errors.comment}}
    </form>
  {{save-cancel}}
</div>
